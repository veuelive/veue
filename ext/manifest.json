{
  "name": "Veue Streamer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension allows you to become a streamer on Veue!",
  "homepage_url": "https://veuelive.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["src/bg/background.js"]
  },
  "browser_action": {
    "title": "Start Stream"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'",
  "permissions": [
    "contentSettings",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "tabCapture",
    "activeTab",
    "http://localhost:3000",
    "https://veuelive.com"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://veuelive.com/videos/streamer",
        "http://localhost:3000/videos/streamer"
      ],
      "js": ["src/dimensionX.js"]
    }
  ]
}
