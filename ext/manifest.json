{
  "name": "Veue Streamer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension allows you to become a streamer on Veue!",
  "homepage_url": "https://veuelive.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/background.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'",
  "permissions": [
    "contentSettings",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "tabCapture",
    "activeTab",
    "http://localhost:3000",
    "https://veuelive.com"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "19": "icons/icon19.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Stream on Veue"
  },
  "content_scripts": [
    {
      "matches": [
        "https://veuelive.com/videos/live",
        "http://localhost:3000/videos/live"
      ],
      "js": ["src/dimensionX.js"]
    }
  ]
}
